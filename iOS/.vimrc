syntax enable
syntax on

set encoding=utf-8
set noswapfile
set nobackup
set incsearch
set hlsearch
set number
set relativenumber
set guifont=CamingoCodeRegular.ttf:h16

set autoindent
set cindent
set tabstop=2
set softtabstop=2
set shiftwidth=2
set shiftround
set expandtab

colorscheme base16-chalk

nnoremap j gj
nnoremap k gk
